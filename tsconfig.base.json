{
  "compilerOptions": {
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": false
  }
}
